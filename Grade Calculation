#include<stdio.h>
#include<conio.h>
void main()
{
int Marks;
printf("Enter marks : ");
scanf("%d",&marks);
if(Marks<=100 && Marks>=85)
{
  printf("\n Grade A");
}
else if (Marks<85 && Marks>=70)
{
  printf("\n Grade B");
}
else if(Marks<70 && Marks>=55)
}
  printf("\n Grade C");
}
else if(Marks<55 && Marks>=40)
{
  printf("\n Grade D");
}
else
{  
  printf("\n Grade F");
}
getch();
}
